{
  "name": "express-typescript-boilerplate",
  "version": "1.0.0",
  "description": "an express-typescript boilerplate",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\""
  },
