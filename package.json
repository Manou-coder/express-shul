    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "concurrently": "^8.2.0",
    "eslint": "^8.42.0",
    "nodemon": "^2.0.22",
    "typescript": "^5.1.3"
  }
}
